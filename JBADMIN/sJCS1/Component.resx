  <data name="infoCust.CommandText" xml:space="preserve">
    <value>select r.RoomerID,PID,RTRIM(RoomerNameC) AS RoomerNameC ,RoomerNameE,dbo.funReturnListData('Gender',r.Gender) as GenderText,
n.NationalityName,r.Birthdate,r.CustNo,r.CustClass,r.CostCenter,c.CustomerID,CustomerShortName
from Roomer r
inner join RoomerInOutLogs l on r.RoomerID = l.RoomerID
inner join CustomerContracts t on l.ContractID = t.ContractID 
inner join Customers c on t.CustomerID =c.CustomerID
inner join Nationality n on r.NationalityID = n.NationalityID
inner join DormRoomBeds d on l.RoomBedID=d.BedID
where l.EffectDate=(Select Max(EffectDate) from RoomerInOutLogs where RoomerID = r.RoomerID ) and l.TranType!=2</value>
