  <data name="BillPayMaster.CommandText" xml:space="preserve">
    <value>Select  b.YearMonth,b.BillPayID,d.RoomerID,r.PID
	,r.RoomerNameC,t.FeeItemAmount as MustPayAmount
	,s.Description as RoomBedID,f.FeeItemID,f.FeeItemName,b.CustomerID,c.CustomerShortName
	 from BillPayMaster b 
		inner join BillPayDetails d on b.BillPayID = d.BillPayID
		inner join BillDetails t on d.BillID=t.BillID and d.InvoiceType=t.InvoiceType----區分=&gt;沖銷方式
		inner join FeeItem f on t.FeeItemID=f.FeeItemID
		inner join Roomer r on d.RoomerID=r.RoomerID
		inner join Customers c on b.CustomerID=c.CustomerID
		inner join RoomerInOutLogs l on d.RoomerID = l.RoomerID
		inner join DormRoomBeds s on l.RoomBedID=s.BedID
		where l.EffectDate=(Select Max(EffectDate) from RoomerInOutLogs where RoomerID = d.RoomerID and TranType !=2) and t.FeeItemAmount!=0</value>
  </data>
