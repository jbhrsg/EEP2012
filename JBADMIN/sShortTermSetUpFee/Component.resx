  <data name="Employer.CommandText" xml:space="preserve">
    <value>SELECT EmployerID,substring(EmployerName,1,4) AS EmployerName
 FROM [60.250.52.106,1433].FWCRM.DBO.EMPLOYER 
WHERE  EMPLOYERID IN (SELECT DISTINCT EMPLOYERID FROM SHORTTERM WHERE (Flowflag='Z' OR Flowflag='P') AND ShortTermTypeID='2') 
ORDER BY  substring(EmployerName,1,4)</value>
  </data>
