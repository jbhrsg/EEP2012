  <data name="SalesMonthTarget.CommandText" xml:space="preserve">
    <value>SELECT AutoKey,[Year],Sales,SalesTypeID,SubClass,
一月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='1' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT1,
二月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='2' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT2,
三月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='3' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT3,
四月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='4' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT4,
五月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='5' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT5,
六月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='6' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT6,
七月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='7' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT7,
八月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='8' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT8,
九月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='9' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT9,
十月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='10' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT10,
十一月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='11' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT11,
十二月,(select SUM(Target) from [SalesDayTarget1] where Year([Date])=s.[Year] and Month([Date])='12' and Sales=s.Sales and SalesTypeID=s.SalesTypeID and SubClass=s.SubClass) as SumDT12,
ISNULL(一月,0)+ISNULL(二月,0)+ISNULL(三月,0)+ISNULL(四月,0)+ISNULL(五月,0)+ISNULL(六月,0)+ISNULL(七月,0)+ISNULL(八月,0)+ISNULL(九月,0)+ISNULL(十月,0)+ISNULL(十一月,0)+ISNULL(十二月,0) as sigma FROM dbo.[SalesMonthTarget] s</value>
  </data>
