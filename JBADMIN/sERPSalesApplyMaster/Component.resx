  <data name="InvoiceList.CommandText" xml:space="preserve">
    <value>SELECT   (Select DLV_IVNO From  [60.250.52.106,1433].NJB.DBO.ltodlver
                    Where Cuno=Custno AND SUBSTRING(dlv_date,1,6)=SalesYM AND  (DLV_L_AMT+DLV_L_TAX=Invoiceamt)) AS  InvoiceNO,
                    CustNO,ApplyEmpID
FROM   dbo.[ERPSalesApplyMaster] 
Where  (FlowFlag='Z' OR FlowFlag='P') AND APPLYDATE&gt;=CONVERT(NVARCHAR(10),GETDATE()-30,111)
                And SalesItemType='+'</value>
