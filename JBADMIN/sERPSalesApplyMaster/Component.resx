  <data name="Sales.CommandText" xml:space="preserve">
    <value>select  ERPSalesMan.SalesID,ERPSalesMan.SalesEmployeeID, 
ERPSalesMan.SalesID+' '+ERPSalesMan.SalesName AS SalesName
