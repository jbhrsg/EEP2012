  <data name="ARStatus.CommandText" xml:space="preserve">
    <value>SELECT '銷貨單號:'+DLVNO+' 銷貨日: '+DLV_DATE+' 銷貨金額： '+convert(char(10), substring(convert(CHAR(10), LTRIM(STR(DLV_L_AMT+DLV_L_TAX,10))), 1, 10))+' 未收金額:'+LTRIM(RTRIM(STR(DLV_L_AMT+DLV_L_TAX-DLV_L_REM,8))) AS Status,
CUNO,
DLVNO,
Convert(char(4),Substring(DLV_DATE,1,3)+1911)+'/'+Substring(DLV_DATE,5,2)+'/'+Substring(DLV_DATE,8,2) AS DLVDATE,
(DLV_L_AMT+DLV_L_TAX) AS DLVAMT
FROM [60.250.52.106,1433].NJB.dbo.LTODLVER
WHERE (DLV_L_AMT+DLV_L_TAX-DLV_L_REM)&gt;=0 AND LEFT(DLV_DATE,3)&gt;=(YEAR(GETDATE())-1911)-1
ORDER BY DLVNO</value>
