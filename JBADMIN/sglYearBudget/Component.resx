  <data name="VoucherMonth.CommandText" xml:space="preserve">
    <value>SELECT DISTINCT YEAR(VoucherDate) AS VoucherYear,Substring(Convert(nvarchar(10),VoucherDate,111),1,7) AS  YM FROM glVoucherMaster 
WHERE Substring(Convert(nvarchar(10),VoucherDate,111),1,7)&lt;=Substring(Convert(nvarchar(10),Getdate(),111),1,7)
ORDER BY YEAR(VoucherDate), YM</value>
  </data>
