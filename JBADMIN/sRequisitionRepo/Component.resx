  <data name="ShortTerm.CommandText" xml:space="preserve">
    <value>select   ShortTerm.ShortTermNO,
             ShortTerm.ShortTermNO+'  :  '+ShortTerm.ShortTermGist +'/' +LTRIM(STR(ShortTerm.ShortTermAmount,8)) as ShortTermDescr,
