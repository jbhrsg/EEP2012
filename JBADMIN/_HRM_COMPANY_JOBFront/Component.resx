  <data name="HRM_COMPANY_JOBFrontFeatured.CommandText" xml:space="preserve">
    <value>SELECT h.*,case when Isnull(f.COMPANY_FrontName,'')='' then f.COMPANY_JOB_FrontName else f.COMPANY_FrontName+'-'+f.COMPANY_JOB_FrontName end as sCOMPANY_FrontName,IsActive
from HRM_COMPANY_JOBFrontFeatured h
	inner join HRM_COMPANY_JOBFront f on h.COMPANY_JOB_ID=f.COMPANY_JOB_ID
order by h.CreateDate desc</value>
  </data>
