  <data name="infoJOBFrontFeatured.CommandText" xml:space="preserve">
    <value>SELECT COMPANY_FrontName,COMPANY_JOB_FrontName,COMPANY_JOB_ID,convert(nvarchar(10),LastUpdateDate,111) as LastUpdateDate
 FROM HRM_COMPANY_JOBFront
where IsActive=1 and Isnull(JobPhotoFile,'')!='' and COMPANY_JOB_ID not in(select COMPANY_JOB_ID from HRM_COMPANY_JOBFrontFeatured)
order by LastUpdateDate desc </value>
  </data>
