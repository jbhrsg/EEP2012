  <data name="HRM_COMPANY_JOBFront.CommandText" xml:space="preserve">
    <value>SELECT [HRM_COMPANY_JOBFront].*,c.ConsultantName,
(select count(*) from HireMePlz.dbo.LetMeJoinPlz where JobId=[HRM_COMPANY_JOBFront].COMPANY_JOB_ID) as iApplyJob
 FROM dbo.[HRM_COMPANY_JOBFront] 
	inner join REC_Consultants c on [HRM_COMPANY_JOBFront].ServiceConsultants=c.ID</value>
  </data>
