  <data name="MyFavJobs.CommandText" xml:space="preserve">
    <value>SELECT j.*,u.UserID as RecUserID,u.NameC+Isnull('/'+g.Gender,'') +Isnull(' '+Cast(DATEDIFF(YEAR,u.Birthday,GETDATE()) as nvarchar(10))+'歲','') as sName,l.EduName,Addr_Country+Isnull(Addr_City,'') as sCurAddress,u.DutyAreas,u.MobileNo
,j.UpdateDate
FROM HireMePlz.dbo.LetMeJoinPlz j
	inner join REC_User u on j.UserIdBack=u.UserID
	left join REC_ZGENType g on u.Gender=g.ID
	left join REC_ZEduLevel l on u.EduID1=l.EduID
where 1=1
order by j.UpdateDate desc</value>
  </data>
