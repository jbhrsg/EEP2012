  <data name="HRM_ATTEND_ABSENT_MINUS.CommandText" xml:space="preserve">
    <value>SELECT HRM_ATTEND_ABSENT_MINUS.*,
HRM_BASE_BASE.EMPLOYEE_CODE,
HRM_BASE_BASE.NAME_C,
HRM_ATTEND_HOLIDAY.HOLIDAY_CODE,
HRM_ATTEND_HOLIDAY.HOLIDAY_CNAME,
HRM_DEPT.DEPT_CODE,
HRM_DEPT.DEPT_CNAME
FROM HRM_ATTEND_ABSENT_MINUS
LEFT JOIN HRM_BASE_BASE ON HRM_ATTEND_ABSENT_MINUS.EMPLOYEE_ID =  HRM_BASE_BASE.EMPLOYEE_ID
LEFT JOIN HRM_ATTEND_HOLIDAY ON HRM_ATTEND_ABSENT_MINUS.HOLIDAY_ID = HRM_ATTEND_HOLIDAY.HOLIDAY_ID
LEFT JOIN HRM_DEPT ON HRM_DEPT.DEPT_ID = dbo.fnHRM_BASE_BASETTS_FieldName(HRM_ATTEND_ABSENT_MINUS.EMPLOYEE_ID,HRM_ATTEND_ABSENT_MINUS.BEGIN_DATE,'DEPT_ID')
WHERE HRM_ATTEND_HOLIDAY.HOLIDAY_FLAG='-'</value>
  </data>
