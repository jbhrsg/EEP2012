  <data name="HRM_ATTEND_OVERTIME_DATA.CommandText" xml:space="preserve">
    <value>SELECT HRM_ATTEND_OVERTIME_DATA.*,
HRM_BASE_BASE.EMPLOYEE_CODE,
HRM_BASE_BASE.NAME_C,
HRM_ATTEND_ROTE.ROTE_CODE,
HRM_ATTEND_ROTE.ROTE_CNAME,
HRM_ATTEND_OVERTIME_CAUSE.OVERTIME_CAUSE_CODE,
HRM_ATTEND_OVERTIME_CAUSE.OVERTIME_CAUSE_CNAME,
HRM_DEPT.DEPT_CODE,
HRM_DEPT.DEPT_CNAME,
HRM_ATTEND_ATTEND_CARD.OFF_TIME_TRAN,
HRM_ATTEND_ATTEND_CARD.ON_TIME_TRAN
FROM HRM_ATTEND_OVERTIME_DATA
LEFT JOIN HRM_BASE_BASE ON HRM_ATTEND_OVERTIME_DATA.EMPLOYEE_ID =  HRM_BASE_BASE.EMPLOYEE_ID
LEFT JOIN HRM_ATTEND_ROTE ON HRM_ATTEND_OVERTIME_DATA.OVERTIME_ROTE_ID = HRM_ATTEND_ROTE.ROTE_ID
LEFT JOIN HRM_ATTEND_OVERTIME_CAUSE ON HRM_ATTEND_OVERTIME_DATA.OVERTIME_CAUSE_ID = HRM_ATTEND_OVERTIME_CAUSE.OVERTIME_CAUSE_ID
LEFT JOIN HRM_DEPT ON HRM_DEPT.DEPT_ID = dbo.fnHRM_BASE_BASETTS_FieldName(HRM_ATTEND_OVERTIME_DATA.EMPLOYEE_ID,HRM_ATTEND_OVERTIME_DATA.OVERTIME_DATE,'DEPT_ID')
LEFT JOIN HRM_ATTEND_ATTEND_CARD ON HRM_ATTEND_OVERTIME_DATA.OVERTIME_DATE = HRM_ATTEND_ATTEND_CARD.CARD_DATE AND HRM_ATTEND_OVERTIME_DATA.EMPLOYEE_ID = HRM_ATTEND_ATTEND_CARD.EMPLOYEE_ID</value>
  </data>
