  <data name="HRM_BASE_BASE.CommandText" xml:space="preserve">
    <value>WITH EMPLOYEE_CTE(DEPT_CODE,DEPT_CNAME, EMPLOYEE_ID,EMPLOYEE_CODE, NAME_C, DEPT_MANAGER, LEVEL)
AS
(
-- Anchor member definition
    SELECT C.DEPT_CODE,C.DEPT_CNAME, A.EMPLOYEE_ID, A.EMPLOYEE_CODE, A.NAME_C, C.DEPT_MANAGER, 1  as LEVEL
    FROM HRM_BASE_BASE AS A
    INNER JOIN DBO.dtHRM_BASE_BASETTS_DateAndMan(NULL,GETDATE()) B ON A.EMPLOYEE_ID = B.EMPLOYEE_ID
    INNER JOIN HRM_DEPT AS C ON B.DEPT_ID = C.DEPT_ID
    WHERE 11=11
    UNION ALL
-- Recursive member definition
    SELECT M.DEPT_CODE,M.DEPT_CNAME, M.EMPLOYEE_ID, M.EMPLOYEE_CODE, M.NAME_C, M.DEPT_MANAGER, LEVEL + 1
    FROM EMPLOYEE_CTE AS A
    INNER JOIN 
    (SELECT C.DEPT_CODE,C.DEPT_CNAME, A.EMPLOYEE_ID, A.EMPLOYEE_CODE, A.NAME_C, C.DEPT_MANAGER
    FROM HRM_BASE_BASE AS A
    INNER JOIN DBO.dtHRM_BASE_BASETTS_DateAndMan(NULL,GETDATE()) B ON A.EMPLOYEE_ID = B.EMPLOYEE_ID
    INNER JOIN HRM_DEPT AS C ON B.DEPT_ID = C.DEPT_ID ) AS M ON A.EMPLOYEE_ID = M.DEPT_MANAGER
    WHERE 12=12
)
-- Statement that executes the CTE
SELECT * FROM EMPLOYEE_CTE
WHERE 13=13 
ORDER BY LEVEL,DEPT_CODE</value>
