  <data name="Employee.CommandText" xml:space="preserve">
    <value>select View_Employee.EmployeeID,
           View_Employee.EmployeeName+' '+Case View_Employee.description when 'JB' then  '' Else 'X' end AS EmployeeName,
View_Employee.DESCRIPTION
           from View_Employee
           where len(View_Employee.EmployeeID)&lt;=5
Order By   EmployeeName
</value>
