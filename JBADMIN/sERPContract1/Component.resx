  <data name="GroupUsers.CommandText" xml:space="preserve">
    <value>with lp(org_no,org_desc,upper_org,lvl) as 
(
SELECT org_no,org_desc,upper_org,0 as lvl FROM [EIPHRSYS].[dbo].[SYS_ORG] where 11=11
union all
SELECT o.org_no,o.org_desc,o.upper_org,lvl+1 FROM [EIPHRSYS].[dbo].[SYS_ORG] o inner join lp l on o.UPPER_ORG=l.org_no
)
SELECT distinct u.USERID,USERNAME FROM[EIPHRSYS].dbo.[USERS] u inner JOIN [EIPHRSYS].[dbo].[USERGROUPS] ug ON ug.USERID=u.USERID inner join [EIPHRSYS].[dbo].[SYS_ORGROLES] ro on ro.ROLE_ID=ug.GROUPID
where DESCRIPTION='JB' and org_no in(
select org_no from lp where  ORG_DESC !='福委會') 
union
select u.USERID,USERNAME  FROM [EIPHRSYS].[dbo].[SYS_ORG] o 
inner join [EIPHRSYS].[dbo].[USERGROUPS] ug on ug.GROUPID=o.ORG_MAN
inner join [EIPHRSYS].[dbo].[USERS] u on u.USERID=ug.USERID
where DESCRIPTION='JB' and org_no in(
select org_no from lp where  ORG_DESC !='福委會') </value>
  </data>
