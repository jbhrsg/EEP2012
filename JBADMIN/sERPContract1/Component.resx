  <data name="ERPContract.CommandText" xml:space="preserve">
    <value>SELECT ContractKey,ParentKey,ContractNO,ContractName,ContractB,ContractClass,ResponsibleDepart,KeepDepart,AssignChecker
,Amount,Remarks,Remarks2,Keeper,Attachment1,Attachment2,Attachment3,Attachment4,Attachment5,FlowFlag,ContinueFlag,CreateBy,CreateDate,IsGuaranty,GuarantyAmount,GuarantyEndDate,GuarantyNO,IsForeignDept
,'' as Button1, Attachment1 as Attachment1d
, Attachment2 as Attachment2d, Attachment3 as Attachment3d
, Attachment4 as Attachment4d, Attachment5 as Attachment5d
,(select count(ContractAlterNO) from ERPContractAlter where ContractNO=ERPContract.ContractNO and (FlowFlag !='X' or FlowFlag is null)) AlterCounts
,BeginDate,EndDate, PhysicalContractNO,RemindDays,SignDate
,(select  CONVERT(varchar(10),sdw.InOutDate,120)+' / '
from SecurityDepositWarrant sdw 
left join SecurityDeposit sd on sd.DepositNO=sdw.DepositNO 
where sd.Flowflag='Z' and sdw.Flowflag='Z' and sd.ContractNO=[ERPContract].ContractNO
FOR XML PATH('')) as InOutDate
,(select  REPLACE(CONVERT(varchar, sdw.WarrantAmount, 1), '.00', '')+' / '
from SecurityDepositWarrant sdw 
left join SecurityDeposit sd on sd.DepositNO=sdw.DepositNO 
where sd.Flowflag='Z' and sdw.Flowflag='Z' and sd.ContractNO=[ERPContract].ContractNO
FOR XML PATH('')) as WarrantAmount
FROM dbo.[ERPContract] order by ContractKey desc</value>
  </data>
