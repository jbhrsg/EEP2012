  <data name="ContractClass.CommandText" xml:space="preserve">
    <value>select ContractClassID,ContractClass,KeepDepart,u.USERNAME,o.ORG_MAN from ERPContractClass e
left join [EIPHRSYS].[dbo].[SYS_ORG] o on e.KeepDepart=o.org_no
left join [EIPHRSYS].[dbo].[USERGROUPS] ug on ug.GROUPID=o.ORG_MAN
left join [EIPHRSYS].[dbo].[USERS] u on u.USERID=ug.USERID</value>
