  <data name="infoBankFees.CommandText" xml:space="preserve">
    <value>SELECT e.EmployeeID,IsNull(e.ResidenceID,'') as ResidenceID,s.YearMonth,Isnull(e.EmployeeEnName+'-','')+Isnull(e.EmployeeTcName,'') as EmployeeName,
Isnull(s.FeeAmount,0) as FeeAmount,Isnull(s2.FeeAmount,0) as FeeAmount2,Isnull(s3.FeeAmount,0) as FeeAmount3,
'第'+Cast(s.TimeLeave as nvarchar)+'期' as sTimeLeave,s.Description,Left(Convert(nvarchar(10),Dateadd(month,1,Right(s.Description,10)),112),6) as sYearMonth 
,'' as Addr,'' as eMail,'' as Description3,'' as Description4,'' as Description5
From Employee e    
inner join EmployeeLogs l on e.EmployeeID=l.EmployeeID
left join EmployeeFees s on e.EmployeeID=s.EmployeeID and s.FeeID='01' and s.IsExcel=0
left join EmployeeFees s2 on e.EmployeeID=s2.EmployeeID and s2.FeeID='06' and s2.YearMonth=Left(Convert(nvarchar(10),Dateadd(month,0,Right(s.Description,10)),112),6) and s2.IsExcel=0
left join EmployeeFees s3 on e.EmployeeID=s3.EmployeeID and s3.FeeID='02' and s3.YearMonth=Left(Convert(nvarchar(10),Dateadd(month,0,Right(s.Description,10)),112),6) and s3.IsExcel=0
		where l.EffectDate=(Select Max(EffectDate) from EmployeeLogs where e.EmployeeID=EmployeeID and IsActive=1) and (l.EffectTypeID=1 or l.EffectTypeID=5 )
group by e.EmployeeID,e.ResidenceID,s.YearMonth,e.EmployeeEnName,e.EmployeeTcName,s.TimeLeave,s.Description,s.FeeAmount,s2.FeeAmount,s3.FeeAmount
having sum(Isnull(s.FeeAmount,0)+Isnull(s2.FeeAmount,0)+Isnull(s3.FeeAmount,0))!=0
order by sYearMonth,EmployeeTcName</value>
  </data>
