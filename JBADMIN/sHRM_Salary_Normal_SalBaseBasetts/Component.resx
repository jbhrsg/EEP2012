  <data name="cb_HRM_SALARY_SALCODE_SalaryApproval.CommandText" xml:space="preserve">
    <value>Select	C.SALARY_ID,
		C.SALARY_CODE,
		C.SALARY_CNAME,
		C.SALARY_ENAME
From	[dbo].[HRM_SALARY_SALCODE] as C
Where	Exists(Select 1 From [dbo].[HRM_SALARY_SALMAPPING_DETAIL] Where SALMAPPING_CODE = 'SalaryApproval' and C.SALARY_ID = SALARY_ID)</value>
  </data>
  <metadata name="HRM_SALARY_SALBASE_BASETTS.TrayLocation" type="System.Drawing.Point, System.Drawing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
