  <data name="infoOrderNo.CommandText" xml:space="preserve">
    <value>select distinct f.OrderNo,r.EmployerName,
	SUM(d.PersonQtyOriginal) as PersonQtyOriginal,f.NationalityID,f.CreateBy,
	(select Isnull(sum(PersonQty),0) from FWCRMIndateCheck where OrderNo=f.OrderNo) as PersonQtyFinal
from FWCRMOrders f
	inner join FWCRMOrdersDetails d on f.OrderNo=d.OrderNo
	inner join View_FWCRMEmployer r on f.EmployerID=r.EmployerID
group by f.OrderNo,r.EmployerName,f.NationalityID,f.CreateBy
order by f.OrderNo desc</value>
  </data>
