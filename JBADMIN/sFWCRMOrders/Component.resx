  <data name="infoReportOrders.CommandText" xml:space="preserve">
    <value>select m.OrderNo,m.OrderType,m.WorkNo,m.EmployerID,r.EmployerName,m.FromOrderNo,
	m.NationalityID,dbo.funReturnReferenceData('nationality',m.NationalityID) as NationalityText,
	d.Item,d.PlanIndate,d.PersonQtyOriginal,d.sgn_no,d.org_okno,d.Notes,
	d.Gender,case d.Gender when 1 then '女' else '男' end as GenderText
from FWCRMOrders m 
inner join FWCRMOrdersDetails d on m.OrderNo=d.OrderNo 
inner join View_FWCRMEmployer r on m.EmployerID=r.EmployerID
</value>
  </data>
