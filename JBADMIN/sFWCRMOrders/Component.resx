  <data name="infoOrg_okno.CommandText" xml:space="preserve">
    <value>
WITH org_oknoData(cus_name,sgn_type,sgn_no,org_okno)
AS
(
	select ltrim(rtrim(c.cus_name))+'-'+org_okno as cus_name,'初招',sgn_no,org_okno
	from [192.168.1.41].lab.dbo.sgn s
		inner join [192.168.1.41].lab.dbo.cus c on s.cus_no=c.cus_no
	where stime_no='A' and org_okno!='' and org_okno not like '%承%'---初招(sgn_no簽約編號,org_okno函號)
	union all
	select ltrim(rtrim(c.cus_name))+'-'+org_okno,'重招',sgn_no,org_okno
	from [192.168.1.41].lab.dbo.sgn s
		inner join [192.168.1.41].lab.dbo.cus c on s.cus_no=c.cus_no
	where stime_no!='A' and org_okno!='' and org_okno not like '%承%'--重招
	union all
	select ltrim(rtrim(c.cus_name))+'-'+cas_no,'遞補',cas_no,cas_no
	from [192.168.1.41].lab.dbo.Back s
		inner join [192.168.1.41].lab.dbo.cus c on s.cus_no=c.cus_no
	where cas_no!='' --遞補
	union all
	select ltrim(rtrim(c.cus_name))+'-'+carry_no,'承接',cas_no,carry_no
	from [192.168.1.41].lab.dbo.cas s
		inner join [192.168.1.41].lab.dbo.cus c on s.cus_no=c.cus_no	
	where carry_no!='' 	--承接
	union all
	select ' 尚未取得',' ',' ',' '
	union all
	select ' 申請中',' ',' ','申請中'
	union all
	select ' 免函號',' ',' ','免函號'
)
-- Statement that executes the CTE
SELECT * FROM org_oknoData
WHERE 11=11 
order by cus_name</value>
  </data>
