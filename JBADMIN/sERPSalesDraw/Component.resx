  <data name="infoCustomersAll.CommandText" xml:space="preserve">
    <value>select '==不拘==' as CustNO,'  ==不拘==' as CustShortName,'' as SalesID 
union all
select top 10 CustNO,CustNO+' : '+CustShortName+'- '+SalesID as CustShortName,SalesID
from  DBO.ERPCustomers
where 1=1
order by CustShortName</value>
