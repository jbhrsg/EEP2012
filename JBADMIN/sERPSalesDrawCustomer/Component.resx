  <data name="InvoiceDetails.CommandText" xml:space="preserve">
    <value>select id.*,c.TelNO,st.SalesTypeName,c.ShortName
,i.InvoiceTypeName,sm.TaxNO 
from InvoiceDetails id
left join Customer c on id.CustomerID=c.CustomerID
left join SalesType st on st.SalesTypeID=id.SalesTypeID
left join InvoiceType i on i.InvoiceTypeID=id.QInvoiceType
left join SalesMaster sm on sm.SalesNO=id.SalesNO
where id.IsActive=1 and id.SalesTypeID in('1','31') 
order by id.SalesDate,InvoiceDate</value>
  </data>
