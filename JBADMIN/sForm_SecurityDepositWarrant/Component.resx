  <data name="Flowflag.CommandText" xml:space="preserve">
    <value>select * from(
select '' as Value,'未起單' as Name
union
select 'N' as Value,'新申請' as Name
union
select 'P' as Value,'流程中' as Name
union
select 'Z' as Value,'結案' as Name
union
select 'X' as Value,'作廢' as Name
) as temp</value>
