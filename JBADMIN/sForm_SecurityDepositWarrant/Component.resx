  <data name="DepositNO.CommandText" xml:space="preserve">
    <value>select * from SecurityDeposit  sd
where  sd.DepositAmount !=ISNULL((
select sum(WarrantAmount) from SecurityDepositWarrant sdw  where sdw.Flowflag in('N','P','Z') and sdw.DepositNO=sd.DepositNO
),0)
and Flowflag ='Z' order by DepositNO desc</value>
