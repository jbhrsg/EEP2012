  <data name="FWCRMStickStatus.CommandText" xml:space="preserve">
    <value> select s.iAutoKey,o.OrderNo,r.EmployerName,o.WorkNo,f.StatusName,s.PersonQty,s.StatusDate,s.StatusResult,s.Notes,o.SalesID,e.NAME_C
 from FWCRMStickStatus s
	inner join FWCRMOrders o  on s.OrderNo=o.OrderNo
	inner join dbo.View_FWCRMEmployer r on o.EmployerID=r.EmployerID
	inner join FWCRMSetStatus f on s.StatusID=f.iAutoKey
	left join [JBHR_EEP].dbo.HRM_BASE_BASE e on o.SalesID=e.EMPLOYEE_CODE
order by o.SalesID,o.OrderNo desc,s.iAutoKey desc</value>
  </data>
