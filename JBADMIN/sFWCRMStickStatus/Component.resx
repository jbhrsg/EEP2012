  <data name="FWCRMOrderList.CommandText" xml:space="preserve">
    <value>WITH OrdersTable(OrderNo,OrderType,OrderStatus,sup_cname,CloseType,EmployerName,PersonQtyOriginal,PersonQty,SalesID,NAME_C,WorkNo,WorkImg,NationalityID,NationalityName,iStickStatus,flowflag) as
(
	SELECT distinct o.OrderNo,o.OrderType,o.OrderStatus,sup_cname,CloseType,r.EmployerName
		,(Select sum(PersonQtyOriginal) from FWCRMOrdersDetails where OrderNo=o.OrderNo)
		,(Select Isnull(SUM(PersonQty),0) from FWCRMIndateCheck where OrderNo=o.OrderNo)
		,o.SalesID,e.NAME_C,o.WorkNo,o.WorkImg,o.NationalityID,y.NationalityName,(select COUNT(*) from FWCRMStickStatus where OrderNo=o.OrderNo),o.flowflag
	FROM FWCRMOrders o
	inner join dbo.View_FWCRMEmployer r on o.EmployerID=r.EmployerID
	inner join FWCRMNationality y on o.NationalityID=y.AutoKey	
	left join [JBHR_EEP].dbo.HRM_BASE_BASE e on o.SalesID=e.EMPLOYEE_CODE	
	group by o.SalesID,e.NAME_C,o.WorkNo,o.WorkImg,o.NationalityID,y.NationalityName,r.EmployerName,o.OrderNo,o.OrderType,o.OrderStatus,sup_cname,CloseType,flowflag
)

Select *,PersonQtyOriginal-PersonQty as QtyResult,Case when flowflag='X' then '作廢' when CloseType=1 then '作廢' else '' end as flowflagText
From OrdersTable
order by OrderNo</value>
