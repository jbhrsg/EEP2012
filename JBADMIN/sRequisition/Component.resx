  <data name="Organization.CommandText" xml:space="preserve">
    <value>SELECT E.ORG_NO,E.ORG_DESC,E.ORG_KIND,E.UPPER_ORG,CostCenterID
FROM EIPHRSYS.dbo.sys_org AS  E
WHERE (E.Upper_Org='10000' OR E.Upper_Org='13000'  OR  E.ORG_NO='10000' OR E.ORG_NO='99999')
ORDER BY E.ORG_NO</value>
