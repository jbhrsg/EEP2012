  <data name="ReportJobRequire.CommandText" xml:space="preserve">
    <value>Select j.JobDeclareDate,j.HunterID,h.HunterName,c.CustomerGrade,j.CustID
		,c.CustShortName,c.IndustryType,i.IndCategory,j.OrderID
		,j.JobFunctionID,f.FunctionName,j.JobID,j.JobName,j.JobNeedCount		
		,Isnull(j.JobRequirement,'')+Isnull(j.JobTerms,'') as JobRequirement,'性別:'+g.Gender +',年齡:'+Case Cast(j.JobAge1 as nvarchar)+'~'+Cast(j.JobAge2 as nvarchar) when '15~99' then '不拘' else  Cast(j.JobAge1 as nvarchar)+'~'+Cast(j.JobAge2 as nvarchar) end as Age
		,Isnull(u.EduName,'')+Isnull(','+t.SubjectName,'')+Case j.EduDepart when '' then '' else ','+j.EduDepart end as EduDepart		
		,dbo.funReturnHUserLanguageString(JobLangNeedType,'',j.JobID) as LanguageString
		,case Isnull(j.JobSkill,'')+Isnull(j.JobLicense,'')+Isnull(j.JobAttitude,'') when '' then '' else '3.'+Isnull(j.JobSkill+CHAR(10),'')+Isnull(' '+j.JobLicense+CHAR(10),'')+Isnull(' '+j.JobAttitude,'') end as JobLicense
		,j.JobWorkContent,j.JobFare,Isnull(z.JobAreaName,'')+Isnull(j.JobWorkArea,'') as sJobWorkArea,j.JobNotes
		,c.SalesTeamID,s.SalesTeamName
		,j.JobDateStd,j.JobCloseDate
	from HUT_Job j 
		inner join HUT_Customer c  on j.CustID=c.CustID
		left join HUT_Hunter h on j.HunterID=h.ID
		left join HUT_IndCategory i on c.IndustryType=i.ID
		left join HUT_Function f on j.JobFunctionID=f.FunctionID
		left join HUT_ZGENType g on j.JobGender=g.ID
		left join HUT_ZEduLevel u on j.EduLevelID=u.EduID
        left join HUT_EduSubject t on j.EduSubject=t.ID       
        left join HUT_ZJobArea z on j.JobArea=z.JobAreaName
        left join HUT_SalesTeam s on j.SalesTeamID=s.ID
order by h.HunterName,j.CreateDate,c.CustShortName</value>
  </data>
