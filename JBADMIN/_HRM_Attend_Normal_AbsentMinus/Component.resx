  <data name="HRM_ATTEND_ABSENT_MINUS_LOG.CommandText" xml:space="preserve">
    <value>SELECT CASE HRM_ATTEND_ABSENT_MINUS_LOG.LOG_STATE
WHEN 'I' THEN '新增'
WHEN 'M' THEN '修改'
ELSE '刪除' END AS LOG_STATE_NAME,HRM_ATTEND_ABSENT_MINUS_LOG.*,
HRM_BASE_BASE.EMPLOYEE_CODE,
HRM_BASE_BASE.NAME_C,
HRM_ATTEND_HOLIDAY.HOLIDAY_CODE,
HRM_ATTEND_HOLIDAY.HOLIDAY_CNAME,
HRM_DEPT.DEPT_CODE,
HRM_DEPT.DEPT_CNAME,
USERS.USERNAME
FROM HRM_ATTEND_ABSENT_MINUS_LOG
LEFT JOIN USERS on HRM_ATTEND_ABSENT_MINUS_LOG.LOG_USER=USERS.USERID
LEFT JOIN HRM_BASE_BASE ON HRM_ATTEND_ABSENT_MINUS_LOG.EMPLOYEE_ID =  HRM_BASE_BASE.EMPLOYEE_ID
LEFT JOIN HRM_ATTEND_HOLIDAY ON HRM_ATTEND_ABSENT_MINUS_LOG.HOLIDAY_ID = HRM_ATTEND_HOLIDAY.HOLIDAY_ID
LEFT JOIN HRM_DEPT ON HRM_DEPT.DEPT_ID = dbo.fnHRM_BASE_BASETTS_FieldName(HRM_ATTEND_ABSENT_MINUS_LOG.EMPLOYEE_ID,HRM_ATTEND_ABSENT_MINUS_LOG.BEGIN_DATE,'DEPT_ID')
WHERE HRM_ATTEND_HOLIDAY.HOLIDAY_FLAG='-'</value>
