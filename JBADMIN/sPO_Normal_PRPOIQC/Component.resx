  <data name="PODelivery.CommandText" xml:space="preserve">
    <value>SELECT dl.*,i.ItemName,i.ItemID,'' as ItemName1,ds.PurVendor,dl.TotalPrice+OtherFee+AcceptanceTax as TotalAmount  FROM dbo.[PODelivery] dl
left join dbo.PODetails ds on dl.ItemNO=ds.ItemNO and dl.PONO=ds.PONO
