  <data name="PODetailsHistory.CommandText" xml:space="preserve">
    <value>SELECT 
       d.[ItemID]
	  ,i.ItemName
      ,[ItemSpec]
      ,[PurPrice]
	  ,[PurQty]
      ,[OtherFee]
      ,[PurVendor]
	  ,[VendShortName]
	  ,m.[ApplyDate]
  FROM [PODetails] d
  left join [POMaster] m on m.PONO=d.PONO
  left join [Item] i on d.ItemID=i.ItemID
  left join [Vendors] v on v.VendID=d.PurVendor
  where m.Flowflag='Z' and 11=11
  order by m.[ApplyDate] desc</value>
  </data>
